    <servlet-class>com.controller.ProductJsonController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProductJsonController</servlet-name>
    <url-pattern>/ProductJsonController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GetProductInfo</display-name>
